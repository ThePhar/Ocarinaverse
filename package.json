{
  "name": "ocarinaverse",
  "version": "0.0.0",
  "description": "An alternative multi-world implementation for The Legend of Zelda - Ocarina of Time: Randomizer.",
  "author": "Zach Parks <zach@alliware.com>",
  "license": "MIT",
  "private": true,
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts,json",
    "ignore": [
      "src/**/*.spec.ts"
    ],
    "exec": "npx ts-node ./src/index.ts"
  },
  "scripts": {
    "build": "rimraf dist && tsc",
    "watch": "nodemon"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^15.12.5",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.4"
  }
}
